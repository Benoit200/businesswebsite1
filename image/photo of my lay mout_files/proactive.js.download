function flyoutOpen() {
	$(".ochat_flyout").addClass("flyout-show");
	setTimeout(function () {
		$(".ochat_flyout").addClass("flyout-fadein")
	}, 400);
}
flyoutOpen();

function flyoutClose() {
	$(".ochat_flyout").removeClass("flyout-fadein");
	setTimeout(function () {
		$(".ochat_flyout").removeClass("flyout-show")
	}, 800)
}

$(document).on("click", "a#ochat-flyoutclose", function (a) {
	var selector = '.ochat_slideout .ochat_slidew2 li';
	flyoutClose()
	$(selector).removeClass('active');
	a.preventDefault();
	return !1
});

var chat_url = 'https://netsuite-salechat.custhelp.com/app/chat/chat_launch/';
var ns_web_page_url = "Incident.CustomFields.c.ns_web_page_url/" + encodeURIComponent(window.location.href);
chat_url = chat_url + ns_web_page_url;

$(document).on("click", ".procta", function (a) {
	$("#chatiframe").attr('src', chat_url);
	flyoutClose();
	a.preventDefault();
	$(".ochat_modal").addClass("chat-show chat-fadein chat_dock whtx");
	$(".ochat_modalw2").addClass("w2active");
	$("#ochat_sales").addClass("iframechat-show iframechat-fadein");
	return !1;
})

